// test for loops

main: function void () = {
	i: integer;
	j: integer;
	for(i = 0; i<10; i++){
		print "counting up:\t",i,"\n";
	}
	for(i = 10; i>0; i--){
		print "counting down:\t",i,"\n";
	}
	for(i = 0; i>10; i=i+2){
		print "counting by 2:\t",i,"\n";
	}
	for(i = 0; i < 5; i++){
		for(j = 0; j < 5; j++){
			print "nested\t";
		}
		print "\n";
	}
}
